  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DBMaintenance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Manutenzioni_NominativiInterni.Nominativo, Manutenzioni.Data, SUM(Manutenzioni.Ore_numeric) AS Ore, 
                         Manutenzioni_TipologiePersonale.Descrizione AS TipologiaPersonale
FROM            Manutenzioni INNER JOIN
                         Manutenzioni_NominativiInterni ON Manutenzioni.NominativoInterniId = Manutenzioni_NominativiInterni.Id INNER JOIN
                         Manutenzioni_TipologiePersonale ON Manutenzioni.TipologiaPersonaleId = Manutenzioni_TipologiePersonale.id
GROUP BY Manutenzioni_NominativiInterni.Nominativo, Manutenzioni.Data, Manutenzioni_TipologiePersonale.Descrizione
HAVING        (Manutenzioni.Data BETWEEN @start_date AND @end_date)</CommandText>
      </Query>
      <Fields>
        <Field Name="Nominativo">
          <DataField>Nominativo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Data">
          <DataField>Data</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Ore">
          <DataField>Ore</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TipologiaPersonale">
          <DataField>TipologiaPersonale</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dates">
      <Query>
        <DataSourceName>DBMaintenance</DataSourceName>
        <CommandText>select  cast(convert(nvarchar(8), getdate()-1, 112) as datetime) as start_date,
 cast(convert(nvarchar(8), getdate(), 112) as datetime) as end_date</CommandText>
      </Query>
      <Fields>
        <Field Name="start_date">
          <DataField>start_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="end_date">
          <DataField>end_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
